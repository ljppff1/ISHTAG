<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
        android:background="@color/white"
    
    android:orientation="vertical"
    android:layout_height="fill_parent"
     >
     
    <RelativeLayout 
        android:layout_width="fill_parent"
        android:layout_height="58dp"
        android:background="@color/black"
        android:paddingTop="5dp"
        android:id="@+id/mRlse"
        android:paddingBottom="5dp"
        >
        <RelativeLayout 
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="@color/white"
            
            >
            <ImageView 
                android:paddingTop="4dp"
                android:paddingBottom="4dp"
                android:layout_width="50dp"
                android:paddingLeft="10dp"
                android:layout_height="50dp"
                android:id="@+id/mIvt1"
                android:layout_centerVertical="true"
                android:src="@drawable/t26"
                />
            <TextView 
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:text="@string/a57"
                android:layout_toRightOf="@id/mIvt1"
                android:id="@+id/mEttt1"
                android:layout_centerVertical="true"
                android:layout_marginLeft="15dp"
                android:textColor="@color/aamy1"
                android:textSize="@dimen/smallc"
                />
        </RelativeLayout>
    </RelativeLayout>
    <LinearLayout 
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:orientation="vertical"
        android:background="@color/white"
        >
                
        <RelativeLayout 
            android:layout_width="fill_parent"
            android:layout_marginTop="15dp"
            android:layout_height="wrap_content"
            >
            <RelativeLayout 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                
                android:layout_centerInParent="true"
                >
                             <TextView 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/a281"
                android:gravity="top"
                android:id="@+id/mTvattep"
               android:textSize="@dimen/smalle"
                android:textColor="@color/black"
                />
                
        <TextView 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:id="@+id/mTvww1"
       android:layout_toRightOf="@id/mTvattep"
                android:layout_marginLeft="@dimen/ma1"
        android:text="@string/a58"
        android:textSize="@dimen/smallc"
        android:textColor="@color/black"
        />
        </RelativeLayout>
        </RelativeLayout>
        <ImageView 
        android:layout_width="fill_parent"
        android:layout_height="10dp"
        android:src="@drawable/t50"
        />
        <RelativeLayout 
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="@color/white"
            >
              <GridView 
                  android:layout_margin="0.5dp"
      android:layout_width="fill_parent"
      android:layout_height="fill_parent"
      android:id="@+id/mGvm1"
      android:verticalSpacing="0.5dp"
      android:horizontalSpacing="0.5dp"
      android:numColumns="4"
      ></GridView>
        </RelativeLayout>
    </LinearLayout>
<RelativeLayout 
    android:layout_width="fill_parent"
    android:layout_height="55dp"
    android:layout_gravity="bottom"
    android:background="@color/white"
    >
      <LinearLayout 
        android:layout_width="fill_parent"
        android:layout_height="44dp"
        android:layout_centerInParent="true"
        android:orientation="horizontal"
        >
               <View 
                        android:layout_width="0dp"
            android:layout_weight="2"
            android:layout_height="35dp"
            
            />
                <ImageView 
            android:layout_width="44dp"
            android:layout_height="44dp"
            android:src="@drawable/aaad1"
            android:id="@+id/mTIvt41f"
            />
                
               <View 
                        android:layout_width="0dp"
            android:layout_weight="0.3"
            android:layout_height="35dp"
            
            />

               <ImageView
                   android:id="@+id/mTIvt41g"
            android:layout_width="44dp"
            android:layout_height="44dp"
                   android:src="@drawable/aaad2" />
   <View 
                        android:layout_width="0dp"
            android:layout_weight="0.3"
            android:layout_height="35dp"
            
            />
                              <ImageView 
            android:layout_width="44dp"
            android:layout_height="44dp"
            android:src="@drawable/t13r"
            android:id="@+id/mTIvt41h1"
            />   <View 
                        android:layout_width="0dp"
            android:layout_weight="0.3"
            android:layout_height="35dp"
            
            />
                              <ImageView 
            android:layout_width="44dp"
            android:layout_height="44dp"
            android:src="@drawable/aaad61"
            android:id="@+id/mTIvt41i"
            />  <View 
                        android:layout_width="0dp"
            android:layout_weight="2"
            android:layout_height="35dp"
            
            />
                              
    </LinearLayout>
</RelativeLayout>
    
</LinearLayout>
